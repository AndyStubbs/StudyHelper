{
	"tabWidth": 4,
	"useTabs": true,
	"semi": true,
	"singleQuote": false,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"arrowParens": "always",
	"endOfLine": "lf",
	"printWidth": 100,
	"overrides": [
		{
			"files": "*.py",
			"options": {
				"tabWidth": 4
			}
		},
		{
			"files": ["*.js", "*.jsx", "*.ts", "*.tsx"],
			"options": {
				"tabWidth": 4
			}
		},
		{
			"files": ["*.html", "*.css"],
			"options": {
				"tabWidth": 4
			}
		}
	]
}
